msg.setting.shariff.orientation.vertical-lg=Vertikal ab LG
msg.setting.shariff.orientation.vertical-md=Vertikal ab MD
msg.setting.shariff.orientation.vertical-sm=Vertikal ab SM
msg.setting.shariff.orientation.vertical-xl=Vertikal ab XL
msg.setting.shariff.service.mail=E-Mail
msg.setting.shariff.text='Teilen' text
msg.setting.shariff.text.help=Legt fest ob neben dem Symbol für den Dienst auch ein Text wie z.B. 'Teilen' angezeigt wird.
msg.setting.shariff.text.notext=Nicht anzeigen
msg.setting.shariff.text.verbose=Immer anzeigen
msg.setting.shariff.text.verbose-lg=Ab LG anzeigen
msg.setting.shariff.text.verbose-md=Ab MD anzeigen
msg.setting.shariff.text.verbose-sm=Ab SM anzeigen
msg.setting.shariff.text.verbose-xl=Ab XL anzeigen
msg.setting.shariff.theme=Symbol-Farbe
msg.setting.shariff.theme.colored=Farbe des Dienstes
msg.setting.shariff.theme.grey=Graue Symbole
msg.setting.shariff.theme.help=Legt fest in welcher Farbe die Symbole für die gewählten Dienste angezeigt werden.
msg.setting.shariff.theme.themed=Themenfarbe des Templates
msg.setting.showAuthor=Zeige Verfasser
msg.setting.showAuthor.help=<p>Legt fest, ob der im Inhalt angegebene Verfasser angezeigt wird.</p>
msg.setting.showButtonOnlyForBooking=Button nur wenn buchbar
msg.setting.showButtonOnlyForBooking.help=Der Button zur Anzeige eines Listeneintrags wird nur angezeigt wenn dieser Eintrag buchbar ist.
msg.setting.showCalendar=Kalenderblatt
msg.setting.showCalendar.help=Zeigt ein Kalenderblatt anstelle eines Bildes an.
msg.setting.showCombinedDownloads=Downloads als Liste
msg.setting.showCombinedDownloads.help=<p>Fasst die Links aus den letzten Absätzen zu einer Download-Liste zusammen.</p><p>Dies betrifft nur Absätze die am Ende stehen und ausschließlich einen Link enthalten.</p>
msg.setting.showDescription=Zeige Beschreibung
msg.setting.showDescription.help=Legt fest ob die Beschreibung aus diesen Inhalt angezeigt wird.
msg.setting.showDownloadCopyright=Zeige Copyright
msg.setting.showDownloadCopyright.help=<p>Legt fest ob das Copyright des Downloads angezeigt wird.</p>\n<p>Dieses wird aus der Datei-Eigenschaft 'Copyright' gelesen.</p>
msg.setting.showFacilities=Zeige Einrichtungen
msg.setting.showFacilities.help=Legt fest ob die verfügbaren Einrichtungen angezeigt werden.
msg.setting.showImageCopyright=Zeige Bild-Copyright
msg.setting.showImageCopyright.help=<p>Legt fest ob das Copyright für Bilder als Einblendung angezeigt wird.</p>
msg.setting.showImageCopyright.listTeaser.help=<p>Legt fest ob für Aufmacher-Bilder das Copyright als Einblendung angezeigt wird.</p>
msg.setting.showImageCount=Zeige Bilder-Anzahl
msg.setting.showImageCount.help=Legt fest ob die Gesamtanzahl der Bilder in der Serie angezeigt wird.
msg.setting.showImageLink=Bild verlinken
msg.setting.showImageLink.help=Falls ein Link für das Bild vorhanden ist, so wird das Bild damit verlinkt.
msg.setting.showImageSeriesListCopyright=Zeige Bild-Copyright in Übersicht
msg.setting.showImageSeriesListCopyright.help=<p>Legt fest ob in der Übersichts-Ansicht das Copyright aller Bilder als Einblendung angezeigt wird.</p>
